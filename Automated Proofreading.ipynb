   "source": [
    "# Python Proofreading\n",
    "# pip install lmproof\n",
    "import lmproof\n",
    "def proofread(text):\n",
    "    proofread = lmproof.load(\"en\")\n",
    "    correction = proofread.proofread(text)\n",
    "    print(\"Original: {}\".format(text))\n",
    "    print(\"Correction: {}\".format(correction))\n",
    "    \n",
    "proofread(\"Your Text\")"
   ]
  }
